@article{thomsonGlaciationDestructiveConstructive2010,
  title = {Glaciation as a Destructive and Constructive Control on Mountain Building},
  shorttitle = {Glaciation as a Destructive and Constructive Control on Mountain Building},
  author = {Thomson, S. N. and Brandon, M. T. and Tomkin, J. H. and Reiners, P. W. and Vasquez, C. and Wilson, N. J.},
