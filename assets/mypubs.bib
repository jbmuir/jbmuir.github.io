@article{ringlerRayleighWaveEllipticity2019,
  title = {Rayleigh Wave Ellipticity Measurement Uncertainty across the {{IRIS}}/{{USGS}} and {{New China Digital Seismograph Networks}}},
  author = {Ringler, Adam T and Wilson, David C and Z{\"u}rn, Walter and Anthony, Robert E},
