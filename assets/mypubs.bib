  shorttitle = {Bayesian Inference for Ultralow Velocity Zones in the {{Earth}}'s Lowermost Mantle},
  author = {Pachhai, S. and Tkal{\v c}i{\'c}, H. and Dettmer, J.},
  year = {2014},
  month = nov,
  journal = {Journal of Geophysical Research: Solid Earth},
  volume = {119},
