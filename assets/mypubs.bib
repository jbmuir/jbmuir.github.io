@article{roweFaultRockInjections2012,
  title = {Fault Rock Injections Record Paleo-Earthquakes},
  shorttitle = {Fault Rock Injections Record Paleo-Earthquakes},
  author = {Rowe, C. D. and Kirkpatrick, J. D. and Brodsky, E. E.},
  year = {2012},
  month = jun,
  journal = {Earth and Planetary Science Letters},
  volume = {335},
