@article{boschiStationaryphaseIntegralsCross2015,
  title = {Stationary-Phase Integrals in the Cross Correlation of Ambient Noise},
  shorttitle = {Stationary-Phase Integrals in the Cross Correlation of Ambient Noise},
  author = {Boschi, L. and Weemstra, C.},
  year = {2015},
  month = jun,
  journal = {Reviews of Geophysics},
  volume = {53},
