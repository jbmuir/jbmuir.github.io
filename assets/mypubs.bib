  title = {Crustal Structure and Tectonics from the {{Los Angeles}} Basin to the {{Mojave Desert}}, Southern {{California}}},
  shorttitle = {Crustal Structure and Tectonics from the {{Los Angeles}} Basin to the {{Mojave Desert}}, Southern {{California}}},
  author = {Fuis, G. S. and Ryberg, T. and Godfrey, N. J. and Okaya, D. A. and Murphy, J. M.},
