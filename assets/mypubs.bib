@article{auerSavaniVariableResolution2014,
  title = {Savani: {{A}} Variable Resolution Whole-Mantle Model of Anisotropic Shear Velocity Variations Based on Multiple Data Sets},
  shorttitle = {{\emph{Savani}}},
  author = {Auer, L. and Boschi, L. and Becker, T. W. and {Nissen-Meyer}, T. and Giardini, D.},
