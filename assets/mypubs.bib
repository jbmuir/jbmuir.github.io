  keywords = {aseismic ridges,axis volcanism,continental lithosphere,eastern indian-ocean,elastic thickness,evolution,gravity,hot spot,hotspot interactions,isostasy,ninety-east ridge,ninetyeast ridge,on- and off-ridge,spreading center,worlds oceans},
