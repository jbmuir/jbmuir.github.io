@article{linEikonalTomographySurface2009,
  title = {Eikonal Tomography: Surface Wave Tomography by Phase Front Tracking across a Regional Broad-Band Seismic Array},
  shorttitle = {Eikonal Tomography},
  author = {Lin, Fan-Chi and Ritzwoller, Michael H. and Snieder, Roel},
