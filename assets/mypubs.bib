@article{schlaphorstInvestigationCoreMantle2016,
  title = {Investigation of Core\textendash Mantle Boundary Topography and Lowermost Mantle with {{P4KP}} Waves},
  author = {Schlaphorst, David and Thomas, Christine and Holme, Richard and Abreu, Rafael},
  year = {2016},
