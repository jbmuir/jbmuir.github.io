@article{jinSurfaceWavePhasevelocity2015,
  title = {Surface Wave Phase-Velocity Tomography Based on Multichannel Cross-Correlation},
  shorttitle = {Surface Wave Phase-Velocity Tomography Based on Multichannel Cross-Correlation},
  author = {Jin, G. and Gaherty, J. B.},
  year = {2015},
  month = jun,
  journal = {Geophysical Journal International},
  volume = {201},
