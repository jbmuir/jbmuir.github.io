@article{shenSeismicReferenceModel2016,
  title = {A Seismic Reference Model for the Crust and Uppermost Mantle beneath {{China}} from Surface Wave Dispersion},
  author = {Shen, Weisen and Ritzwoller, Michael H. and Kang, Dou and Kim, YoungHee and Lin, Fan-Chi and Ning, Jieyuan and Wang, Weitao and Zheng, Yong and Zhou, Longquan},
  year = {2016},
