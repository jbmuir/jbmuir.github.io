@article{bowerDynamicOriginsSeismic2013,
  title = {Dynamic Origins of Seismic Wavespeed Variation in {{D}}''},
  author = {Bower, Dan J. and Gurnis, Michael and Sun, Daoyuan},
  year = {2013},
  month = jan,
  journal = {Physics of the Earth and Planetary Interiors},
  volume = {214},
  pages = {74--86},
  issn = {00319201},
  doi = {10.1016/j.pepi.2012.10.004},
  abstract = {The D00 discontinuity is defined by a seismic velocity increase of 1\textendash 3\% about 250 km above the core\textendash mantle boundary (CMB), and is mainly detected beneath locations of inferred paleosubduction. A phase change origin for the interface can explain a triplicated arrival observed in seismic waveform data and is supported by the recent discovery of a post-perovskite phase transition. We investigate the interaction of slabs, plumes, and the phase change within D00 in 2-D compressible convection calculations, and predict waveform complexity in synthetic seismic data. The dynamic models produce significant thermal and phase heterogeneity in D00 over small distances and reveal a variety of behaviors including: (1) distinct pPv blocks separated by upwellings, (2) notches at the top of a pPv layer caused by plume heads, (3) regions of Pv embedded within a pPv layer due to upwellings. Advected isotherms produce complicated thermal structure that enable multiple crossings of the phase boundary. Perturbations to S, SdS, and ScS arrivals (distances {$<$}84 degrees) are linked to the evolutionary stage of slabs and plumes, and can be used to determine phase boundary height and velocity increase, volumetric wavespeed anomaly beneath the discontinuity, and possibly the lengthscale of slab folding near the CMB. Resolving fine-scale structure beneath the interface requires additional seismic phases (e.g., Sd, SKS) and larger distances ({$>$}80 degrees). \'O 2012 Elsevier B.V. All rights reserved.},
  langid = {english},
  file = {/Users/jackbmuir/Documents/Papers/Zotero/storage/3DXNE2FL/Bower et al. - 2013 - Dynamic origins of seismic wavespeed variation in.pdf}
}

