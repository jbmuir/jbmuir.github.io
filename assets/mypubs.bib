@article{pitarkaStrongGroundMotion2019,
  title = {Strong {{Ground Motion Simulation}} for the {{M7}}.1, 2019 {{Ridgecrest California Earthquake}}},
  author = {Pitarka, A. and Graves, R. W. and Rodgers, A. J.},
  year = {2019},
  month = dec,
  journal = {AGU Fall Meeting Abstracts},
  volume = {31},
  abstract = {The M7.1 Ridgecrest earthquake generated good quality strong motion data  on a broad distance range. These data are very useful for testing kinematic rupture generators for strong ground motion simulations of crustal earthquakes in California. In order to investigate the performance of the GP kinematic rupture generator in simulating the recorded ground motion we used the GP to generate a rupture model for the Ridgecrest earthquake. Our model contains a large slip patch that overlaps the background slip. Both the background and large slip areas contain correlated small-scale random slip variations. The location of the large slip patch was constrained by the slip distribution obtained from slip inversions (Dreger 2019). The near-fault (distance {$<$}30km) ground motion was computed using a hybrid method that computes the low frequency and high frequency parts separately, using deterministic and stochastic schemes, respectively. The low frequency part ({$<$} 1Hz) was computed using the FK method and a 1D regional velocity model with a Vs30=500 m/s. Finally the synthetic broad-band acceleration time histories were corrected for site effects using empirical site corrections based on the local Vs30. Using several realizations of the earthquake rupture, we investigated the sensitivity of near-fault ground motion to GP rupture generator parameters, in particular, depth of large slip area, and default parameters such as rise time scaling with slip and depth, and correlation between spatial variations of slip and rise time. In our presentation we will show results of our investigation. This work performed under the auspicesof the U.S. Department of Energy by Lawrence Livermore National Laboratory under Contract DE-AC52-07NA27344},
  keywords = {7299 General or miscellaneous,SEISMOLOGY}
}

