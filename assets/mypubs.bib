  keywords = {geotomoprop},
  file = {/Users/jackbmuir/Documents/Papers/Zotero/storage/BTWTSGS8/Gimbert et al. - 2016 - Subseasonal changes observed in subglacial channel.pdf}
}

@article{girolamiRiemannManifoldLangevin2011,
