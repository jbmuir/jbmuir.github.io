@article{schmandtSeismicArrayConstraints2017,
  title = {Seismic Array Constraints on Reach-Scale Bedload Transport},
  author = {Schmandt, B. and Gaeuman, D. and Stewart, R. and Hansen, S.M. and Tsai, V.C. and Smith, J.},
