@article{koelemeijerNormalModeSensitivity2012,
  title = {Normal Mode Sensitivity to {{Earth}}'s {{D}}" Layer and Topography on the Core-Mantle Boundary: What We Can and Cannot See},
  shorttitle = {Normal Mode Sensitivity to {{Earth}}'s {{D}}" Layer and Topography on the Core-Mantle Boundary: What We Can and Cannot See},
