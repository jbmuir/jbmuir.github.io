}

@article{bergShearVelocityModel2020,
  title = {Shear {{Velocity Model}} of {{Alaska Via Joint Inversion}} of {{Rayleigh Wave Ellipticity}}, {{Phase Velocities}}, and {{Receiver Functions Across}} the {{Alaska Transportable Array}}},
