@article{moraCanEarthHarmonic2015,
  title = {Can the {{Earth}}'s Harmonic Spectrum Be Derived Directly from the Stochastic Inversion of Global Travel-Time Data?},
  author = {Mora, Steve Della and Boschi, Lapo},
