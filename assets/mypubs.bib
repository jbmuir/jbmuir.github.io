@article{penmanRapidSustainedSurface2014,
  title = {Rapid and Sustained Surface Ocean Acidification during the {{Paleocene-Eocene Thermal Maximum}}},
  shorttitle = {Rapid and Sustained Surface Ocean Acidification during the {{Paleocene-Eocene Thermal Maximum}}},
  author = {Penman, D. E. and Honisch, B. and Zeebe, R. E. and Thomas, E. and Zachos, J. C.},
