@article{gimbertSubseasonalChangesObserved2016,
  title = {Subseasonal Changes Observed in Subglacial Channel Pressure, Size, and Sediment Transport: {{SUBGLACIAL HYDROLOGY FROM SEISMIC NOISE}}},
