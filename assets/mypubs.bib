@article{kustowskiShearwaveVelocityStructure2008,
  title = {The Shear-Wave Velocity Structure in the Upper Mantle beneath {{Eurasia}}},
  shorttitle = {The Shear-Wave Velocity Structure in the Upper Mantle beneath {{Eurasia}}},
