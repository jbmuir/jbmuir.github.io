@article{gorszczykGraphSpaceOptimal2021,
  title = {Graph-{{Space Optimal Transport Concept}} for {{Time}}-{{Domain Full}}-{{Waveform Inversion}} of {{Ocean}}-{{Bottom Seismometer Data}}: {{Nankai Trough Velocity Structure Reconstructed From}} a {{1D Model}}},
  shorttitle = {Graph-{{Space Optimal Transport Concept}} for {{Time}}-{{Domain Full}}-{{Waveform Inversion}} of {{Ocean}}-{{Bottom Seismometer Data}}},
  author = {G{\'o}rszczyk, Andrzej and Brossier, Romain and M{\'e}tivier, Ludovic},
