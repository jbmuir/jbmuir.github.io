@article{tesonieroHydrationMarginalBasins2015,
  title = {Hydration of Marginal Basins and Compositional Variations within the Continental Lithospheric Mantle Inferred from a New Global Model of Shear and Compressional Velocity},
  shorttitle = {Hydration of Marginal Basins and Compositional Variations within the Continental Lithospheric Mantle Inferred from a New Global Model of Shear and Compressional Velocity},
  author = {Tesoniero, Andrea and Auer, Ludwig and Boschi, Lapo and Cammarano, Fabio},
  year = {2015},
