@article{schaffImprovementsMagnitudePrecision2014,
  title = {Improvements in Magnitude Precision, Using the Statistics of Relative Amplitudes Measured by Cross Correlation},
  shorttitle = {Improvements in Magnitude Precision, Using the Statistics of Relative Amplitudes Measured by Cross Correlation},
  author = {Schaff, D. P. and Richards, P. G.},
  year = {2014},
