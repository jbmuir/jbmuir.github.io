@article{hariharanEvidenceOvertoneInterference2020,
  title = {Evidence of {{Overtone Interference}} in {{Fundamental}}-{{Mode Rayleigh Wave Phase}} and {{Amplitude Measurements}}},
  author = {Hariharan, Anant and Dalton, Colleen A. and Ma, Zhitu and Ekstr{\"o}m, G{\"o}ran},
  year = {2020},
