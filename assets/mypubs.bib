  shorttitle = {On the {{Implications}} of {{A Priori Constraints}} in {{Transdimensional Bayesian Inversion}} for {{Continental Lithospheric Layering}}},
  author = {Roy, C. and Romanowicz, B. A.},
  year = {2017},
  month = dec,
  journal = {Journal of Geophysical Research: Solid Earth},
  volume = {122},
